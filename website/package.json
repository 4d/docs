{
  "license": "CC-BY-4.0",
  "scripts": {
    "crowdin-upload": "crowdin --config ../crowdin.yaml upload sources --auto-update -b master",
    "crowdin-download": "crowdin --config ../crowdin.yaml download -b master",
    "examples": "docusaurus-examples",
    "syntaxViewPro": "doc_preprocess --path ../docs/ViewPro/ --syntax='./syntax/ViewPro/' && doc_preprocess --path 'translated_docs/*/ViewPro/' --syntax='./syntax/ViewPro/'",
    "syntax": "doc_preprocess --path ../docs/API/ --syntax='./syntax/4D/' --config='../docs/preprocessing.conf' && doc_preprocess --path 'translated_docs/*/API/' --syntax='./syntax/4D/' --config='../docs/preprocessing.conf'",
    "build": "doc_preprocess --path ../docs/ --config='../docs/preprocessing.conf' --output ../docs/ && doc_preprocess --path 'translated_docs/*' --config='../docs/preprocessing.conf' --output translated_docs/ && docusaurus-build",
    "start": "docusaurus-start",
    "startwatch": "doc_preprocess --path ../docs/ --output ../docsPostProcessed/ --watch=true",
    "doc": "doc_preprocess --path ../docs/ --output ../docsPostProcessed/ --config='../docs/preprocessing.conf' --syntax='./syntax/'",
    "publish-gh-pages": "docusaurus-publish",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version"
  },
  "devDependencies": {
    "@4dsas/doc_preprocessing": "1.0.14",
    "docusaurus": "^1.14.6",
    "highlightjs-4d": "^1.0.6"
  }
}
