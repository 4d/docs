{
  "license": "CC-BY-4.0",
  "scripts": {
    "crowdin-upload": "crowdin --config ../crowdin.yaml upload sources --auto-update -b master",
    "crowdin-download": "crowdin --config ../crowdin.yaml download -b master",
    "examples": "docusaurus-examples",
    "prebuild": "node prebuild.js",
    "prestart": "node prebuild.js",
    "prestart_watch": "node prebuild.js",
    "start": "docusaurus start",
    "doc_preprocess": "doc_preprocess",
    "start_watch": "doc_preprocess --settings_type='start_watch'",
    "syntaxViewPro": "doc_preprocess --settings_type='build_syntax_viewPro' && doc_preprocess --settings_type='build_syntax_viewPro_translated'",
    "syntax": "cross-env NODE_OPTIONS='--max-old-space-size=8192' doc_preprocess --settings_type='build_syntax' && doc_preprocess --settings_type='build_syntax_translated'",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' doc_preprocess && doc_preprocess --settings_type='build_versioned_docs' && doc_preprocess --settings_type='build_translate' && docusaurus build",
    "publish-gh-pages": "docusaurus-publish",
    "write-translations": "docusaurus write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "highlightjs-4d": "^1.0.6"
  },
  "dependencies": {
    "@4dsas/doc_preprocessing": "1.1.3",
    "@docusaurus/core": "3.4.0",
    "@docusaurus/plugin-client-redirects": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/theme-mermaid": "3.4.0",
    "@swc/core": "^1.6.5",
    "clsx": "^2.1.1",
    "esbuild-loader": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-gfm": "^4.0.0",
    "swc-loader": "^0.2.6"
  },
  "engines": {
    "node": ">=18.0"
  }
}
